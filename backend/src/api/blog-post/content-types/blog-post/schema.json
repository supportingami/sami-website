{
  "kind": "collectionType",
  "collectionName": "blog_posts",
  "info": {
    "singularName": "blog-post",
    "pluralName": "blog-posts",
    "displayName": "Blog Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Content": {
      "type": "richtext"
    },
    "FeatureImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Title": {
      "type": "string"
    },
    "Summary": {
      "type": "text"
    },
    "DateWritten": {
      "type": "date"
    },
    "Tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blog-tag.blog-tag",
      "mappedBy": "BlogPost"
    }
  }
}
